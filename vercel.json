{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "crons": [
    {
      "path": "/sendNotificationSameDay",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/sendNotification",
      "schedule": "0 15 * * *"
    }
  ]
  }
